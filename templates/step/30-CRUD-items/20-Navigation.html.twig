{# templates/step/04-pagination.html.twig #}

{% extends 'step/layout-pagination.html.twig' %}

{% block content_data %}
	{% for i in 0..7 %}
		{% embed 'step/row-pagination.html.twig' with {'data': dataPagination(i, 10, 70, 10), 'linkExtension': linkExtension} only %}
			{% block content_pagination %}
				{{ dump(linkExtension) }}
				{% include '@MazariniBootstrap/pagination/nav.html.twig' %}
			{% endblock content_pagination %}
		{% endembed %}
	{% endfor %}
{% endblock content_data %}
