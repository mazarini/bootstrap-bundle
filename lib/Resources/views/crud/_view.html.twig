<table class="table">
	<tbody>
		{% block content %}{% endblock content %}
	</tbody>
</table>
{% block edit %}
	{% if data.links.edit.url != '#' %}
		{% include '@MazariniBootstrap/button/link.html.twig' with { 'link': data.links.edit } %}
	{% endif %}
{% endblock edit %}
{% block delete %}
	{% if data.links.delete.url != '#' %}
		{% include '@MazariniBootstrap/button/toggle.html.twig' with { 'label': 'Supprimer', 'target': 'modalDeleteId' } %}
	{% endif %}
{% endblock delete %}
{% block back %}
	{% if data.links.index.url != '#' %}
		{% include '@MazariniBootstrap/button/link.html.twig' with { 'link': data.links.index, 'label': 'Retour'} %}
	{% endif %}
{% endblock back %}
